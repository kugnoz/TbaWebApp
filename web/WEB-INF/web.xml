<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>ServletTbaQREncoder</servlet-name>
        <servlet-class>kr.ac.ajou.dv.tbawebapp.ServletTbaQREncoder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletTbaQREncoder</servlet-name>
        <url-pattern>/qrencode</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletApkQREncoder</servlet-name>
        <servlet-class>kr.ac.ajou.dv.tbawebapp.ServletApkQREncoder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletApkQREncoder</servlet-name>
        <url-pattern>/apkqrcode</url-pattern>
    </servlet-mapping>
</web-app>
